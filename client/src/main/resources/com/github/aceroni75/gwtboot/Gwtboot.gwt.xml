<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='gwtboot'>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.github.aceroni75.gwtboot.GwtbootShared"/>
    <inherits name="com.gwtplatform.dispatch.rest.delegates.ResourceDelegate"/>
    <inherits name='com.gwtplatform.mvp.MvpWithEntryPoint'/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>

    <source path='client'/>

    <extend-configuration-property name="gin.ginjector.modules"
                                   value="com.github.aceroni75.gwtboot.client.ClientModule"/>
</module>